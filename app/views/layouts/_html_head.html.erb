<head>
  <%= title :site => t('application.title'), :separator => '-' %>
  <link rel="shortcut icon" href="<%= image_path('favicon.ico') %>" />
  <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
  <meta http-equiv="content-language" content="ru"/>
  <%= stylesheet_link_tag 'reset', 'pagination', 'application' %>
  <%= javascript_include_tag 'jquery.min', 'jquery_ujs', 'socky.min', 'application' %>
  <%= yield :head %>
  <%= csrf_meta_tags %>
</head>
