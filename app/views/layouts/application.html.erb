<!DOCTYPE html>
<html>
<head>
  <%= render :partial => 'layouts/html_head' %>
  <title>VirtualOffice</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="all">
  <div id="header">
    <%= render :partial => 'layouts/header' %>
    <%#= render :partial => "layouts/navigation" %>
    <%#= render :partial => 'layouts/sub_navigation' %>
  </div>
  <div id="content">
    <%= render :partial => 'layouts/flash' %>
    <h1><%= display_headline %></h1>
    <%= yield %>
  </div>

  <div id="empty"></div>
  <div id="footer">
    <%= render :partial => 'layouts/footer' %>
  </div>
</div>

<%= link_to('Logout', logout_path, :method => :delete) if logged_in? %>
</body>
</html>
