  <%= javascript_include_tag 'jquery-1.4.2.min.js', 'jquery-ui-1.8.2.custom.min.js' %>
  <%= javascript_include_tag :application %>
  <%= stylesheet_link_tag "scaffold", "jquerycss", 'jquery-ui-1.8.2-custom.css' %>
